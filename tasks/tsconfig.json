{
  "ts-node": {
    "swc": true
  },
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "public",
    "outDir": ".",
    "declaration": false,
    "target": "es2021",
    "lib": ["es2021"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "incremental": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "noEmitOnError": false,
    "strict": true,
    "newLine": "LF",
    "noEmit": true,
    "noImplicitReturns": true,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
  },
}